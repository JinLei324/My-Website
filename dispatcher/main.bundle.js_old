    DispatcherHomeComponent.prototype.driverTransfer = function (m, value1, value2) {
        //       m -> socket response
        this.ref.detectChanges();
        //  value1 -> From which array u should find the provider  
        //  value2 -> To which array u should shift the provider
        //checking if provider name is in  provider array                     
        //  value3 -> value as per driverSorting function in ngOnInIt for that req array
        //  value4 -> Marker Array , where u want to add new marker 
        //  value5 -> marker image  
        // demote1 -> Marker Array from u want to delete the marker
        if (value1.findIndex(function (item) { return item._id == m._id; }) >= 0) {
            var indVal = value1.findIndex(function (item) { return item._id == m._id; });
            value1.splice(value1.findIndex(function (item) { return item._id == m._id; }), 1);
            var findVal = value2.findIndex(function (item) { return item._id == m._id; });
            if (findVal >= 0) {
                value2.splice(findVal, 1);
            }
            value2.unshift(m);
            value2.lastUpdatedMinutes = "Last updated " + Math.round(m.LastOnline / 60) + "min ago";
        }
        else {
            //if the provider is not in any list then he will be added to the list where u wanted to shift him 
            value2.unshift(m);
        }
        this.ref.detectChanges();
    };
    DispatcherHomeComponent.prototype.providerChanges = function (m) {
        if (this.selectedStoreId == 0) {
            var dtype = parseInt(sessionStorage.getItem('driverType'));
            (m.accountType == 1 && dtype == 2) ? this.driverFilter(m) :
                (m.accountType == 1 && dtype == 1) ? '' : this.driverSwitch(m);
            // this.availablePro ,this.busyPro , this.loggedOutPro , this.offlinePro
        }
        else if (this.selectedStore.driverType == 1 && (this.selectedStoreId == m.storeId)) {
            (m.accountType != 1) ? this.driverSwitch(m) : '';
        }
        else if (this.selectedStore.driverType == 2) {
            (m.accountType == 1) ? this.driverStoreFilter(m) : '';
        }
        this.setdefaultImage();
        this.ref.detectChanges();
    };
