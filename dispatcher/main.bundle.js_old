    // provider movement changes online,offline,busy,loggedout
    GodsviewComponent.prototype.providerChanges = function (m) {
        console.log("available -///outside//////////////////->", this.available);
        switch (parseInt(m.status)) {
            // move provider to Online
            case 3:
                if (this.available.findIndex(function (item) { return item._id == m._id; }) < 0) {
                    console.log("available -/////////////////////->", this.available);
                    console.log("m._id -////////////////////////->", m._id);
                    this.selectedIndexChange(1);
                    this.markerAvailable = [];
                    //check in offline
                    if (this.offline.findIndex(function (item) { return item._id == m._id; }) >= 0) {
                        this.offlineProCount--;
                        this.onlineProCount++;
                        this.driverTransfer(m, this.offline, this.available, this.availablePoints, this.markerAvailable, this.markerOffline);
                    }
                    else if (this.inactive.findIndex(function (item) { return item._id == m._id; }) >= 0) {
                        console.log("check in inactive");
                        this.onlineProCount++;
                        this.inactiveProCount--;
                        this.driverTransfer(m, this.inactive, this.available, this.availablePoints, this.markerAvailable, this.markerInactive);
                    }
