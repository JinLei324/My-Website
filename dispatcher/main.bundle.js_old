n mat-button class=\"tinyButton\" *ngIf=\"!(cartOrderType == 3)\" type=\"button\" (click)=\"patchCartFromAddedItems(item,1)\">\r\n                                            <i class=\"material-icons primary\">&#xE146;</i>\r\n                                        </button>-->\r\n                                    </div>\r\n                                    <div class=\"col-md-3 padding-0 text-center\" *ngIf=\"item.finalPrice > 0\">\r\n                                        <span *ngIf=\"cartArray && cartArray.cart\">{{currencySymbols}}</span>{{item.quantity * item.finalPrice| number:'2.1-1'}}\r\n                                    </div>\r\n                                    <div class=\"col-md-3 padding-0 text-center\" *ngIf=\"item.finalPrice == 0\"></div>\r\n                                    \r\n                                    <div class=\"col-md-1 padding-0\" [hidden]=\"cartArray.cart[0].products.length == 1\">\r\n                                        <a (click)=\"deleteEachOrderInCart(item.childProductId, item.unitId, item)\">\r\n                                            <i class=\"fa fa-trash\" aria-hidden=\"true\"></i>\r\n                                        </a>\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"col-md-12\" *ngIf=\"item.addOnAvailable == 1\">\r\n                                    <i style=\"font-size:10px;color:blue;cursor:pointer;\" (click)=\"showCustomizeAddonsInCart(item)\">Customize</i>\r\n                                </div>\r\n                                <div class=\"col-md-12\" *ngFor=\"let selectedItemAddon of item.selectedAddOns\">\r\n                                    <i>- {{selectedItemAddon.name}}</i>\r\n                                    <i style=\"float:right;padding-right:5px;\"><span>{{currencySymbols}}</span>{{item.quantity * selectedItemAddon.price | number:'2.1-1'}}</i>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n\r\n                    <hr/>\r\n\r\n                    <div class=\"col-md-12 allowSpace\">\r\n                        <div class=\"col-md-1 padding-0 pTop15\">\r\n                            <i class=\"fa fa-money billIcon\" aria-hidden=\"true\"></i>\r\n                        </div>\r\n                        <div class=\"col-md-6 padding-0 pTop15\">Select Payment*</div>\r\n                        <div class=\"col-md-5 padding-0 semiSelector\">\r\n                            <mat-form-field>\r\n                                <mat-select placeholder=\"Pay by\" [formControl]=\"JobPaymentType\" [(ngModel)]=\"paymentTypeValue\">\r\n                                    <mat-option [value]=\"2\">Cash</mat-option>\r\n                                    <!-- <mat-option [value]=\"1\" [disabled]=\"CardSelectDisable\">Card</mat-option> -->\r\n                                </mat-select>\r\n                                <mat-error *ngIf=\"JobPaymentType.invalid\">{{getPaymentError()}}</mat-error>\r\n                            </mat-form-field>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"col-md-12\" [hidden]=\"!paymentTypeValue || paymentTypeValue == 2\">\r\n                        <div class=\"col-md-1 padding-0 pTop15\">\r\n                            <i class=\"fa fa-credit-card-alt billIcon\" aria-hidden=\"true\"></i>\r\n                        </div>\r\n                        <div class=\"col-md-6 padding-0 pTop15\">Select Card*</div>\r\n                        <div class=\"col-md-5 padding-0 semiSelector\">\r\n                            <mat-form-field>\r\n                                <mat-select placeholder=\"Select Card\" [formControl]=\"cardSelect\" [(ngModel)]=\"selectedCard\">\r\n                                    <mat-option *ngFor=\"let card of savedCards\" [value]=\"card.id\">\r\n            