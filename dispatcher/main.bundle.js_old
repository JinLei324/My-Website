   </div>\r\n                    </div>\r\n                    <!--<hr>-->\r\n\r\n                    <div class=\"col-md-12 padding-0 allowSpace\">\r\n                        <div class=\"col-md-1 padding-0\"></div>\r\n                        <div class=\"col-md-5 padding-1\"></div>\r\n                        <div class=\"col-md-3 padding-0\"></div>\r\n                        <div class=\"col-md-3 padding-1 text-right tTotal\" *ngIf=\"this.storeTypeVal != 7\">\r\n                            <b *ngIf=\"(cartArray && cartArray.cart && cartArray.finalTotalIncludingTaxes)\">\r\n                                <sup>{{currencySymbols}}</sup>{{cartArray.finalTotalIncludingTaxes | number:'2.1-2'}}</b>\r\n                        </div>\r\n                        <div class=\"col-md-3 padding-1 text-right tTotal\" *ngIf=\"this.storeTypeVal == 7 && deliveryCharge\">\r\n                            <b>\r\n                                <sup>{{currencySymbols}}</sup>{{deliveryCharge | number:'2.1-2'}}\r\n                            </b>\r\n                        </div>\r\n            \r\n                    </div>\r\n                    <hr>\r\n                    <div class=\"col-md-12 padding-0\" id=\"estimatedPackageValue\">\r\n                        <div class=\"col-md-12 backgroundFade\">\r\n                            <h5>ESTIMATED PACKAGE VALUE</h5>\r\n                        </div>\r\n                        <div class=\"col-md-12 bglight\">\r\n                            <label class=\"dudeLabel\">\r\n                                <i class=\"fa fa-inr\" aria-hidden=\"true\"></i>\r\n                            </label>\r\n                            <mat-form-field class=\"dudeSub\">\r\n                                <input matInput (keydown)=\"dontAllowAlphabets($event)\" [(ngModel)]=\"estimatedPackageValue\"/>\r\n                            </mat-form-field>\r\n                        </div>\r\n                    </div>\r\n                    <!-- <hr> -->\r\n  