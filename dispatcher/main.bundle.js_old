   <div class=\"pad10 productComments\">\r\n                                                <img src=\"{{orderDetails.driverDetails.image}}\" width=\"30\" height=\"30\"/>\r\n                                                <span>\r\n                                                    {{orderDetails.driverDetails.fName + \" \" + orderDetails.driverDetails.lName}}({{orderDetails.driverDetails.countryCode + orderDetails.driverDetails.mobile}})\r\n                                                </span>\r\n                                            </div>\r\n                                        </div>\r\n                                    </div>\r\n                                    <hr>\r\n                                </div>\r\n\r\n                                <div class=\"col-md-12 mrg5 positionStatic\">\r\n                                    <div class=\"col-md-12 positionStatic\">\r\n                                        <div class=\"col-md-12 subheader positionStatic\">\r\n                                            <b>Order Details:</b>\r\n                                        </div>\r\n                                        <div class=\"col-md-12 padding-0 positionStatic\">\r\n                                            <div class=\"pad10\">\r\n                                                <div class=\"col-md-12 padding-0 mrg5 positionStatic\">\r\n                                                    <div class=\"col-md-5 positionStatic\">\r\n                                                        <b>Item Name</b>\r\n                                                    </div>\r\n                                                    <div class=\"col-md-1 text-right positionStatic\">\r\n                                                        <b>Quantity</b>\r\n                                                    </div>\r\n                                                    <div [hidden]=\"orderDetails.storeType == 5 || orderDetails.storeType == 7\" class