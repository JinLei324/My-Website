    // SEARCH PLACE ON MAP
    GodsviewComponent.prototype.searchPlace = function () {
        this.searchPoints = [];
        this.searchPoints.forEach(function (marker) {
            marker.setMap(null);
        });
        this.origin_autocomplete.bindTo('bounds', this.map1);
        var place = this.origin_autocomplete.getPlace();
        var map = this.map1.setOptions({
            center: { lat: place.geometry.location.lat(), lng: place.geometry.location.lng() },
            zoom: 13,
            minZoom: 1
        });
        this.searchPoints.push({ "lat": place.geometry.location.lat(), "lng": place.geometry.location.lng() });
        for (var i = 1; i < this.markerSearch.length; i++) {
            this.markerSearch[i].setMap(null);
        }
        this.markerSearch[0].setMap(this.map1);
    };
