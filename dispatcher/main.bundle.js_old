        </ng-container>\r\n\r\n        <!-- Weight Column -->\r\n        <ng-container matColumnDef=\"Mobile\">\r\n            <mat-header-cell *matHeaderCellDef> Mobile Number </mat-header-cell>\r\n            <mat-cell *matCellDef=\"let element\"> {{element.customerDetails.countryCode + element.customerDetails.mobile}} </mat-cell>\r\n        </ng-container>\r\n\r\n        <!-- Symbol Column -->\r\n        <ng-container matColumnDef=\"Email\">\r\n            <mat-header-cell *matHeaderCellDef> Email Address </mat-header-cell>\r\n            <mat-cell *matCellDef=\"let element\"> {{element.customerDetails.email}} </mat-cell>\r\n        </ng-container>\r\n\r\n        <ng-container matColumnDef=\"Driver\">\r\n            <mat-header-cell *matHeaderCellDef style=\"text-align:center\"> Driver Name </mat-header-cell>\r\n            <mat-cell *matCellDef=\"let element\" style=\"text-align:center\">\r\n                <span *ngIf=\"element.driverDetails\">{{element.driverDetails.fName}} {{element.driverDetails.lName}}</span>\r\n                <span *ngIf=\"!element.driverDetails\">Driver Not Found </span>\r\n            </mat-cell>\r\n        </ng-container>\r\n        <!--element.status == 15-->\r\n        <ng-container matColumnDef=\"Status\">\r\n            <mat-header-cell *matHeaderCellDef style=\"text-align:center\"> Status </mat-header-cell>\r\n            <mat-cell *matCellDef=\"let element\" style=\"text-align:center\">\r\n                <span>{{element.statusMsg}}</span>\r\n            </mat-cell>\r\n        </ng-container>\r\n\r\n        <ng-container matColumnDef=\"OrderTime\">\r\n            <mat-header-cell *matHeaderCellDef style=\"text-align:center\"> Order At </mat-header-cell>\r\n            <mat-cell *matCellDef=\"let element\" style=\"text-align:center;\"> {{element.bookingDate | date:'HH:mm, MMM dd'}} </mat-cell>\r\n        </ng-container>\r\n        <ng-container matColumnDef=\"DeliveryTime\">\r\n            <mat-header-cell *matHeaderCellDef style=