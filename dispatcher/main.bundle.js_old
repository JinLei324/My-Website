    DispatcherHomeComponent.prototype.hasCommonElement = function (arr1, arr2) {
        var bExists = false;
        $.each(arr2, function (index, value) {
            if ($.inArray(value, arr1) != -1) {
                bExists = true;
            }
            if (bExists) {
                return false; //break
            }
        });
        return bExists;
    };
    DispatcherHomeComponent.prototype.driverSwitch = function (m) {
        console.log("-----------------m value-------------------", m);
        switch (parseInt(m.status)) {
            // move provider to offline
            case 4:
                var indx = this.availableProMarker.findIndex(function (item) { return item.id == m.driverId; });
                console.log("this.availableProMarker---offline---driverSwitch ", this.availableProMarker);
                if (indx !== -1) {
                    this.availableProMarker[indx].setMap(null);
                }
                if (this.offlinePro.findIndex(function (item) { return item._id == m._id; }) < 0) {
                    // this.toastr.info('Went offline', 'Driver : ' + m.name);
                    this.offlineCount++;
                    if (this.availablePro.findIndex(function (item) { return item._id == m._id; }) >= 0) {
                        this.availableCount--;
                        this.driverTransfer(m, this.availablePro, this.offlinePro);
                    }
                    else if (this.loggedOutPro.findIndex(function (item) { return item._id == m._id; }) >= 0) {
                        this.driverTransfer(m, this.loggedOutPro, this.offlinePro);
                        this.loggedOutCount--;
                    }
                    else if (this.inactivePro.findIndex(function (item) { return item._id == m._id; }) >= 0) {
                        this.driverTransfer(m, this.inactivePro, this.offlinePro);
                        this.inactiveCount--;
                    }
                    else {
                        this.offlinePro.unshift(m);
                    }
                }
                break;
            // move provider to online
            case 3:
                var indwwx = this.availableProMarker.findIndex(function (item) { return item.id == m.driverId; });
                console.log("this.availableProMarker---offline---driverSwitch ", this.availableProMarker);
                if (indwwx !== -1) {
                    this.availableProMarker[indwwx].setMap(this.storeMap);
                }
                else {
                    console.log("this.availableProMarker---online---driverSwitch ", this.availableProMarker);
                    var bounds = new google.maps.LatLngBounds();
                    // if ((m.status == 4) || (m.status == 21 || m.inDispatch == false)) {
                    var micon = {
                        url: "https://lh6.ggpht.com/2kNvSeSXkJGXR-A9RBEq3qAMM7rdq7EQTf96fAoOf7H3EP2w4ZVmnOIN0p47AnBgAgU=w300",
                        scaledSize: new google.maps.Size(40, 40),
                        origin: new google.maps.Point(0, 0),
                        anchor: new google.maps.Point(0, 0) // anchor
                    };
                    this.availableProMarker.push(new google.maps.Marker({
                        position: { lat: m.latitude, lng: m.longitude },
                        map: this.storeMap,
                        icon: micon,
                        animation: google.maps.Animation.DROP,
                        id: m.driverId
                    }));
                    var name = "";
                    var phone = '';
                    name = m.name;
                    phone = m.phone;
                    var infowindow = new google.maps.InfoWindow();
                    var value3 = this.availableProMarker;
                    var contentString;
                    contentString = '<b>Driver Name: </b>' + name + '<br /><b>Driver Phone:</b>  ' + phone;
                    google.maps.event.addListener(m, 'mouseover', (function (value3, contentString, infowindow) {
                        return function () {
                            infowindow.setContent(contentString);
                            infowindow.open(this.storeMap, value3);
                        };
                    })(m, contentString, infowindow));
                    google.maps.event.addListener(m, 'mouseout', (function (value3, contentString, infowindow) {
                        return function () {
                            infowindow.close();
                        };
                    })(m, contentString, infowindow));
                    var avlloc = new google.maps.LatLng(m.latitude, m.longitude);
                    bounds.extend(avlloc);
                    this.ref.detectChanges();
                    // }
                }
                if (this.availablePro.findIndex(function (item) { return item._id == m._id; }) < 0) {
                    //   this.toastr.info('<span style="color: red">Came online</span>');
                    this.availableCount++;
                    // this.toastr.info('Came online', 'Driver : ' + m.name);
                    if (this.offlinePro.findIndex(function (item) { return item._id == m._id; }) >= 0) {
                        this.offlineCount--;
                        this.driverTransfer(m, this.offlinePro, this.availablePro);
                    }
                    else if (this.loggedOutPro.findIndex(function (item) { return item._id == m._id; }) >= 0) {
                        this.driverTransfer(m, this.loggedOutPro, this.availablePro);
                        this.loggedOutCount--;
                    }
                    else if (this.inactivePro.findIndex(function (item) { return item._id == m._id; }) >= 0) {
                        this.driverTransfer(m, this.inactivePro, this.availablePro);
                        this.inactiveCount--;
                    }
                    else {
                        this.availablePro.unshift(m);
                    }
                }
                break;
            //move provider to loggedout
            case 8:
                var ind = this.availableProMarker.findIndex(function (item) { return item.id == m.driverId; });
                if (ind !== -1) {
                    this.availableProMarker[ind].setMap(null);
                }
                if (this.loggedOutPro.findIndex(function (item) { return item._id == m._id; }) < 0) {
                    // this.toastr.info('has been logged out', 'Driver : ' + m.name);
                    this.loggedOutCount++;
                    if (this.availablePro.findIndex(function (item) { return item._id == m._id; }) >= 0) {
                        this.driverTransfer(m, this.availablePro, this.loggedOutPro);
                        this.availableCount--;
                    }
                    else if (this.offlinePro.findIndex(function (item) { return item._id == m._id; }) >= 0) {
                        this.offlineCount--;
                        this.driverTransfer(m, this.offlinePro, this.loggedOutPro);
                    }
                }
                break;
            case 9:
                // inactivePro
                var index = this.availableProMarker.findIndex(function (item) { return item.id == m.driverId; });
                if (index !== -1) {
                    this.availableProMarker[index].setMap(null);
                }
                if (this.inactivePro.findIndex(function (item) { return item._id == m._id; }) < 0) {
                    // this.toastr.info('is Inactive', 'Driver : ' + m.name);
                    this.inactiveCount++;
                    if (this.availablePro.findIndex(function (item) { return item._id == m._id; }) >= 0) {
                        this.driverTransfer(m, this.availablePro, this.inactivePro);
                        this.availableCount--;
                    }
                    else if (this.offlinePro.findIndex(function (item) { return item._id == m._id; }) >= 0) {
                        this.offlineCount--;
                        this.driverTransfer(m, this.offlinePro, this.inactivePro);
                    }
                }
                break;
        }
    };
